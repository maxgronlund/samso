# show weather from DMI
class Admin::<%= file_name.classify %>Module < ApplicationRecord
  has_many :page_col_modules, as: :moduleable
  include PageColConcerns

  def page_module
    PageModule.find_by(
      moduleable_type: 'Admin::<%= file_name.classify %>Module',
      moduleable_id: id
    )
  end
end
