    .my-2.my-lg-0
      ul.navbar-nav
        - if  admin?

          li.nav-item
            = render 'shared/navbar_link',
              link_name: 'Admin',
              link_url: admin_index_url
        li.nav-item.dropdown
          a#dropdown01.nav-link.dropdown-toggle aria-expanded="false" aria-haspopup="true" data-toggle="dropdown" href="http://example.com"  Min Konto
          .dropdown-menu.dropdown-menu-right aria-labelledby="dropdown01"
            - if user_signed_in?
              = link_to 'Logout', destroy_user_session_path, method: :delete, class: 'dropdown-item'
              = link_to 'Min profil', user_path(current_user), class: 'dropdown-item'

            - else
              = link_to 'Login', new_user_session_path, class: 'dropdown-item'
              = link_to t(".sign_up"), new_registration_path('user'), class: 'dropdown-item'
